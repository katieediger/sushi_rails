<div class="row">
<h3>Valid COUNTER Connections</h>
</div>
<div class="row header">
  <div class="col-lg-1">
    <h6>Database Name</h6>
  </div>
  <div class="col-lg-2">
    <h6>Endpoint</h6>
  </div>
  <div class="col-lg-2">
    <h6>Customer ID</h6>
  </div>
  <div class="col-lg-2">
    <h6>Requestor ID</h6>
  </div>
  <div class="col-lg-1">
    <h6>Report Start</h6>
  </div>
  <div class="col-lg-1">
    <h6>Report End</h6>
  </div>
  <div class="col-lg-1">
    <h6>Password</h6>
  </div>
  <div class="col-lg-2">
    <h6>Actions</h6>
  </div>
</div>
<% @validsushi.each do |s| %>
<div class="row data">
  <div class="col-lg-1">
    <p><%= s.name %></p>
  </div>
  <div class="col-lg-2">
    <p><%= s.endpoint %></p>
  </div>
  <div class="col-lg-2">
    <p><%= s.cust_id %></p>
  </div>
  <div class="col-lg-2">
    <p><%= s.req_id %></p>
  </div>
  <div class="col-lg-1">
    <p><%= s.report_start %></p>
  </div>
  <div class="col-lg-1">
    <p><%= s.report_end %></p>
  </div>
  <div class="col-lg-1">
    <p><%= s.password %></p>
  </div>
  <div class="col-lg-2">
    <%= link_to "Import", {:controller => :validsushi, :action => :import, :id => (s.id)}, class: 'btn btn-primary'%>
  </div>
</div>
<% end %>
