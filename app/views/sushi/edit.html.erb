<h1> Edit Sushi Connection </h1>
<%= form_for (@sushi) do |f| %>
  <label>
    Name
    <%= f.text_field :name %>
  </label>
  <label>
    Endpoint
    <%= f.text_field :endpoint %>
  </label>
  <label>
    Customer ID
    <%= f.text_field :cust_id %>
  </label>
  <label>
    Requestor ID
    <%= f.text_field :req_id %>
  </label>
  <label>
    Report Start
    <%= f.text_field :report_start %>
  </label>
  <label>
    Report End
    <%= f.text_field :report_end %>
  </label>
  <label>
    <%= f.submit "Edit" %>
  </label
<% end %>
